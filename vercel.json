{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "out"
      }
    }
  ],
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "^/(.*)\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot)$",
      "dest": "/$1.$2"
    },
    {
      "src": "^/(.*)/$",
      "dest": "/$1/index.html"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ]
}
